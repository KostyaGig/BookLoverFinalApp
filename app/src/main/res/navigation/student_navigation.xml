<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/fragment_root_student_book">
    <fragment
        android:id="@+id/fragment_student_progress"
        android:name="com.example.bookloverfinalapp.app.ui.student_screens.screen_progress.FragmentStudentProgress"
        android:label="fragment_student_progress"
        tools:layout="@layout/fragment_student_progress" >
        <action
            android:id="@+id/action_fragment_student_progress_to_fragmentAdminUploadPdf"
            app:destination="@id/fragmentAdminUploadPdf" />
    </fragment>
    <fragment
        android:id="@+id/fragment_student_profile"
        android:name="com.example.bookloverfinalapp.app.ui.student_screens.screen_profile.FragmentStudentProfile"
        android:label="fragment_student_profile"
        tools:layout="@layout/fragment_student_profile">
        <action
            android:id="@+id/action_fragment_student_profile_to_fragmentStudentEditProfile"
            app:destination="@id/fragmentStudentEditProfile" />
    </fragment>
    <fragment
        android:id="@+id/fragment_student_setting"
        android:name="com.example.bookloverfinalapp.app.ui.student_screens.screen_setting.FragmentStudentSetting"
        android:label="fragment_student_setting"
        tools:layout="@layout/fragment_student_setting" />
    <fragment
        android:id="@+id/fragmentStudentChapterBook"
        android:name="com.example.bookloverfinalapp.app.ui.student_screens.screen_chapter_book.ui.FragmentStudentChapterBook"
        android:label="fragment_student_chapter_book"
        tools:layout="@layout/fragment_student_chapter_book">
        <argument
            android:name="book"
            app:argType="com.example.bookloverfinalapp.app.models.StudentBook" />
        <action
            android:id="@+id/action_fragmentStudentChapterBook_to_fragmentStudentReader"
            app:destination="@id/fragmentStudentReader" />
    </fragment>
    <fragment
        android:id="@+id/fragmentStudentReader"
        android:name="com.example.bookloverfinalapp.app.ui.student_screens.screen_reader.FragmentStudentReader"
        android:label="fragment_reader"
        tools:layout="@layout/fragment_reader">
        <argument
            android:name="book"
            app:argType="com.example.bookloverfinalapp.app.models.StudentBook" />
        <argument
            android:name="startPage"
            app:argType="integer" />
        <argument
            android:name="lastPage"
            app:argType="integer" />
        <argument
            android:name="chapter"
            app:argType="integer" />
        <action
            android:id="@+id/action_fragmentStudentReader_to_fragmentStudentBookQuestion"
            app:destination="@id/fragmentStudentBookQuestion" />
    </fragment>
    <fragment
        android:id="@+id/fragmentStudentEditProfile"
        android:name="com.example.bookloverfinalapp.app.ui.student_screens.screen_edit_profile.FragmentStudentEditProfile"
        android:label="FragmentStudentEditProfile" />
    <fragment
        android:id="@+id/fragment_root_student_book"
        android:name="com.example.bookloverfinalapp.app.ui.student_screens.screen_book_root.FragmentRootStudentBook"
        android:label="fragment_student_book_root"
        tools:layout="@layout/fragment_student_book_root">
        <action
            android:id="@+id/action_fragment_root_student_book_to_fragmentStudentChapterBook"
            app:destination="@id/fragmentStudentChapterBook" />
        <action
            android:id="@+id/action_fragment_root_student_book_to_fragmentStudentBookDetails2"
            app:destination="@id/fragmentStudentBookDetails" />
    </fragment>
    <fragment
        android:id="@+id/fragmentStudentBookDetails"
        android:name="com.example.bookloverfinalapp.app.ui.student_screens.screen_book_details.FragmentStudentBookDetails"
        android:label="fragment_student_book_details"
        tools:layout="@layout/fragment_student_book_details">
        <argument
            android:name="book"
            app:argType="com.example.bookloverfinalapp.app.models.Book" />
    </fragment>
    <fragment
        android:id="@+id/fragmentStudentBookQuestion"
        android:name="com.example.bookloverfinalapp.app.ui.student_screens.screen_questions.FragmentStudentBookQuestion"
        android:label="fragment_student_book_question"
        tools:layout="@layout/fragment_student_book_question">
        <argument
            android:name="book"
            app:argType="com.example.bookloverfinalapp.app.models.StudentBook" />
        <argument
            android:name="chapter"
            app:argType="integer" />
        <action
            android:id="@+id/action_fragmentStudentBookQuestion_to_fragmentStudentChapterBook"
            app:destination="@id/fragmentStudentChapterBook"
            app:popUpTo="@+id/fragment_root_student_book" />
    </fragment>
    <fragment
        android:id="@+id/fragmentAdminUploadPdf"
        android:name="com.example.bookloverfinalapp.app.ui.admin_screens.FragmentAdminUploadPdf"
        android:label="fragment_admin_upload_pdf"
        tools:layout="@layout/fragment_admin_upload_pdf" />
</navigation>